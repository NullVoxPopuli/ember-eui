{{#let (arg-or-default @id (unique-id)) as |theId|}}
  <EuiFormRow
    class={{@rowClasses}}
    @extra={{this.rowExtra}}
    @labelType={{@labelType}}
    @display={{@display}}
    @hasEmptyLabelSpace={{@hasEmptyLabelSpace}}
    @fullWidth={{@fullWidth}}
    @hasChildLabel={{@hasChildLabel}}
    @label={{this.label}}
    @labelAppend={{@labelAppend}}
    @id={{theId}}
    @isInvalid={{this.isInvalidAndTouched}}
    @error={{this.validationErrorMessages}}
    @helpText={{@helpText}}
    @errorClasses={{@errorClasses}}
    {{did-insert this.setValidationMessages}}
    {{did-update this.didUpdateValue @value}}
  >
    <EuiDualRange
      class={{@fieldClasses}}
      form={{@formId}}
      @value={{@value}}
      @fullWidth={{@fullWidth}}
      @isInvalid={{this.isInvalidAndTouched}}
      @levels={{@levels}}
      @showLabels={{@showLabels}}
      @showInput={{@showInput}}
      @tickInternval={{@tickInternval}}
      @ticks={{@ticks}}
      aria-label={{@ariaLabel}}
      @disabled={{@disabled}}
      @compressed={{@compressed}}
      @hasFocus={{@hasFocus}}
      @showRange={{@showRange}}
      @showTicks={{@showTicks}}
      @readOnly={{@readOnly}}
      @id={{theId}}
      @step={{@step}}
      @showValue={{@showValue}}
      @onChange={{this.handleChange}}
      @max={{@max}}
      @isLoading={{@isLoading}}
      @min={{@min}}
      @icon={{@icon}}
      @side={{@side}}
      @inputRef={{@inputRef}}
      @controlOnly={{@controlOnly}}
      @autoSize={{@autoSize}}
      @isPrependProvided={{has-block "prepend"}}
      @isAppendProvided={{has-block "append"}}
      @isFakeMinBlock={{not (has-block "min")}}
      @isFakeMaxBlock={{not (has-block "max")}}
      autofocus={{@autofocus}}
      ...attributes
      {{on "blur" (fn this.setIsTouched true)}}
    >
      <:prepend as |classes|>
        {{yield classes to="prepend"}}
      </:prepend>
      <:min>
        {{yield to="min"}}
      </:min>
      <:max>
        {{yield to="max"}}
      </:max>
      <:append as |classes|>
        {{yield classes to="append"}}
      </:append>
    </EuiDualRange>
  </EuiFormRow>
{{/let}}
