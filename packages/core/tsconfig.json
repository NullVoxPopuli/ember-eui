{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "declarationDir": "./dist",
    "declaration": true,
    "declarationMap": true,
    "paths": {
      "@glimmer/tracking": [
        "../../node_modules/ember-cached-decorator-polyfill",
        "../../node_modules/@glimmer/tracking/dist/types"
      ],
      "@ember/destroyable": ["../../../node_modules/ember-destroyable-polyfill"],
      ".": ["./index.ts"],
      "./*": ["./*"],
    }
  },
  "include": [
    "src/**/*",
    "types/**/*"
  ]
}
