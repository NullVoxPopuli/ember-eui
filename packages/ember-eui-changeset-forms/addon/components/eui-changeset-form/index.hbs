<EuiForm
  @tagName="form"
  {{did-insert this.didInsertForm}}
  {{on "submit" this.onSubmit}}
>
  <:content>
    <EuiChangesetForm::Context
      @changeset={{@changeset}}
      @fullWidth={{@fullWidth}} as |FormContext|
    >
      {{yield FormContext @changeset}}
    </EuiChangesetForm::Context>
  </:content>
</EuiForm>