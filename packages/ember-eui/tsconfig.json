{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "dummy/tests/*": ["tests/*"],
      "@glimmer/tracking": [
        "../../node_modules/ember-cached-decorator-polyfill",
        "../../node_modules/@glimmer/tracking/dist/types"
      ],
      "dummy/*": ["tests/dummy/app/*", "app/*"],
      "ember-eui": ["addon"],
      "ember-eui/*": ["addon/*"],
      "ember-eui/test-support": ["addon-test-support"],
      "ember-eui/test-support/*": ["addon-test-support/*"],
      "*": ["types/*"]
    }
  },
  "include": [
    "app/**/*",
    "addon/**/*",
    "tests/**/*",
    "types/**/*",
    "test-support/**/*",
    "addon-test-support/**/*"
  ]
}
