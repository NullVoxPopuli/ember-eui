{{yield (hash Description=(component "docfy-demo/description" id=@id))}}

<EuiSplitPanel::Outer
  id={{@id}}
  class="docfy-demo guideSection euiSplitPanel"
  @hasBorder={{true}}
  @paddingSize="none"
  @hasShadow={{false}}
  @grow={{false}}
  ...attributes
>
  {{yield
    (hash
      Example=(component "docfy-demo/example")
      Snippet=(component "docfy-demo/snippet")
      Snippets=(component "docfy-demo/snippets")
    )
  }}
</EuiSplitPanel::Outer>